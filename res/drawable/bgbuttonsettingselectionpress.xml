<?xml version="1.0" encoding="utf-8"?>
<shape
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:shape="rectangle">

	<gradient
		android:startColor="#ffd1ff"
		android:centerColor="#ffd1ff"
		android:endColor="#ffd1ff"
		android:angle="90"/>

	<stroke
		android:width="1dp"
		android:color="#A0000000"/>

	<padding
		android:top="5dp"
		android:bottom="5dp"
		android:left="5dp"
		android:right="5dp"/>

	<corners
		android:radius="7dp"/>

</shape>
